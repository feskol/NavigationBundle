{
    "name": "feskol/navigation-bundle",
    "description": "This bundle provides an integration of the feskol/php-navigation library.",
    "keywords": [
        "navigation",
        "navigation-active-state",
        "navigation-helper",
        "menu"
    ],
    "homepage": "https://github.com/feskol/NavigationBundle",
    "authors": [
        {
            "name": "Festim Kolgeci",
            "email": "festim.kolgeci@pm.me"
        }
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "feskol/php-navigation": "^3.0",
        "symfony/framework-bundle": "^6.1 | ^7.0",
        "twig/twig": "^2.13|^3.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Feskol\\Bundle\\NavigationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feskol\\Bundle\\NavigationBundle\\Tests\\": "tests/"
        }
    }
}
